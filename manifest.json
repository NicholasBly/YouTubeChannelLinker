{
  "manifest_version": 3,
  "name": "YouTube Channel Link Fix",
  "version": "1.0",
  "description": "Makes channel names in recommended videos link to the channel page",
  "permissions": ["scripting"],
  "host_permissions": ["https://www.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}